<main>
  <div id="carouselFadeExampleIndicators" class="carousel slide carousel-fade" data-ride="carousel">
  <div class="carousel-inner" role="listbox">
    <div class="carousel-item active">
      <%= image_tag "shinee.jpg", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "redvelvet.jpg", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "nctdream.jpg", class: "d-block w-100" %>
    </div>
