<div class="container">
  <h2>ジャンル一覧・追加</h2>
    <%= form_with model: @category, url: admin_category_path(@category), class: "form-inline", local: true do |f| %>
      <%= f.label :name, "ジャンル名" %>
      <%= f.text_field :name, class: "mx-2" %>
      <%= f.submit "変更を保存", class: "sbtn btn-4 m-0" %>
    <% end %>
  </div>
</div>